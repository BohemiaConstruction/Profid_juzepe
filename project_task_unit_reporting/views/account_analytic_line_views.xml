<odoo>
    <record id="view_analytic_line_form_inherit_unit_qty" model="ir.ui.view">
        <field name="name">account.analytic.line.form.inherit.unit.qty</field>
        <field name="model">account.analytic.line</field>
        <field name="inherit_id" ref="account.view_analytic_line_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='timesheet_ids']/tree/field[@name='unit_amount']" position="after">
                <field name="unit_qty"/>
            </xpath>
        </field>
    </record>
</odoo>
