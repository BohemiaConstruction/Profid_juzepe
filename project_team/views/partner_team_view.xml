<odoo>
    <record id="view_res_partner_form_inherit_project_team" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.project.team</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_registry']" position="after">
                <field name="project_team_id" string="Project Team"/>
            </xpath>
        </field>
    </record>
</odoo>