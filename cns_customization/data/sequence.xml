<odoo>
    <data noupdate="1">
        <record id="seq_res_partner_cislo_clena_text" model="ir.sequence">
            <field name="name">Partner Number Sequence</field>
            <field name="code">res.partner.cislo.clena</field>
            <field name="prefix">%(y)s</field>  <!-- Použití posledních dvou číslic roku -->
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range">True</field>  <!-- Povolení rozsahů dat pro prefix -->
        </record>
    </data>
</odoo>
